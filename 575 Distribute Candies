class Solution {
    public int distributeCandies(int[] candies) {
        int res;
        int size = candies.length/2;
        Set<Integer> set = new HashSet();
        for(int i:candies)
            set.add(i);
        
        if(set.size()>size)
            res=size;
        else
            res=set.size();
        
        return res;
    }
}
